    "jsonwebtoken": "^7.4.1",
    "mongoosastic": "^4.3.0",
    "mongoose": "^4.10.4",
    "morgan": "^1.8.2",
    "rxjs": "^5.1.0",
    "tether": "1.4.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
