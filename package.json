  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod --dbpath ./_mongodb/data\" \"npm run elastic-search\" \"npm run pug-it:w\" \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\" ",
    "prod": "concurrently \"mongod --dbpath ./_mongodb/data\" \"npm run elastic-search\" \"npm run pug-it:w\" \"ng build -aot -prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "pug-it": "pug ./templates -o ./client/app -P -O \"{doctype: 'html'}\"",
    "pug-it:w": "pug ./templates -o ./client/app -P -w -O \"{doctype: 'html'}\"",
    "elastic-search": "./_elasticsearch/bin/elasticsearch"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
